{
  "rules": {
    ".read": true,
    ".write": true,
    "targets": {
      "$target": {
        ".validate": "newData.isString() && newData.val().contains('..') === false"
      }
    },
    "votes": {
      "$name": {
        ".validate": "newData.hasChildren(['value', 'date'])",
        "value": {
          ".validate": "newData.isString()"
        },
        "date": {
          ".validate": "newData.isString()"
        }
      }
    }
  }
}
